{
  "name": "n8n-pretty-chat",
  "version": "1.0.2",
  "description": "Drop-in replacement for @n8n/chat with message-style conversation experience inspired by Julian Garnier's work.",
  "main": "dist/chat.umd.js",
  "module": "dist/chat.es.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/chat.es.js",
      "require": "./dist/chat.umd.js",
      "types": "./dist/types/index.d.ts"
    },
    "./style.css": "./dist/style.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build && tsc --emitDeclarationOnly --outDir dist/types",
    "preview": "vite preview",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "n8n",
    "chat",
    "chatbot",
    "ai",
    "widget",
    "webhook",
    "conversational",
    "bubble",
    "animation"
  ],
  "author": "schaferjart",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/schaferjart/n8n-pretty-chat.git"
  },
  "homepage": "https://github.com/schaferjart/n8n-pretty-chat#readme",
  "bugs": {
    "url": "https://github.com/schaferjart/n8n-pretty-chat/issues"
  },
  "devDependencies": {
    "typescript": "^5.3.0",
    "vite": "^5.0.0"
  }
}
